#Импортирование библиотеки PyGame
from pygame import *

#Создание окна приложения
width = 700
height = 500
display.set_caption("Ping-pong")
window = display.set_mode((width, height))
window.fill(125, 160, 250)
window.blit(width, height)

run = True
finish = False
while run:
    for e in event.get():
        if e.type == QUIT:
            run = False:





    display.update()
    time.delay(50)
